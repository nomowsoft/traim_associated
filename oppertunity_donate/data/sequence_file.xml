    <field name="implementation">no_gap</field>
    <field name="prefix">PN</field>
    <field name="padding">5</field>
    <field name="number_next">1</field>
    <field name="number_increment">1</field>
  </record>
  <record id="seq_transfer_fincial" model="ir.sequence">
    <field name="name">transfer_fincial</field>
    <field name="implementation">no_gap</field>
    <field name="code">transfer.fincial.num</field>
    <field name="implementation">no_gap</field>
    <field name="prefix">TF</field>
    <field name="padding">5</field>
    <field name="number_next">1</field>
    <field name="number_increment">1</field>
  </record>

</odoo>
